{
    "manifest_version": 2,
    "name": "URL Sticky Note",
    "description": "A space for locally stored notes on every web page.",
    "version": "0.1",
    "permissions": [],
    "browser_action": {
        "default_icon": {
            "19": "resources/stickynote_icon_19.png",
            "38": "resources/stickynote_icon_38.png",
            "76": "resources/stickynote_icon_76.png"
        },
        "default_title": "URL Sticky Note",
        "default_popup": "html/stickynote_popup.html"
    }
}
